<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="string" column="role" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="num" type="integer" column="num" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emploi" type="string" column="emploi" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valide" type="integer" column="valide" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
